Sub macrosOn()
'///Assigned to "On" cell on various sheets////////////////////////////////////////////////////////////////////////////////
'///Turns on Macros////////////////////////////////////////////////////////////////////////////////////////////////////////

    Range("AA1").Value = "On"
    
End Sub


Sub macrosOff()
'///Assigned to "Off" cell on various sheets///////////////////////////////////////////////////////////////////////////////
'///Turns off Macros///////////////////////////////////////////////////////////////////////////////////////////////////////

    Range("AA1").Value = ""
    
End Sub